    <title>{pageTitle}</title>

    <meta name="title" content="◕ᴗ◕ Dragon Odile">
    <meta name="description" content={pageDescription}>
    
    <meta property="og:site_name" content="◕ᴗ◕ Dragon Odile">
    <meta property="og:title" content="◕ᴗ◕ Dragon Odile">
    <meta property="og:type" content="website">
    <!-- <meta property="og:url" content="https://bearblog.dev"> -->
    <meta property="og:description" content={pageDescription}>
    <meta property="og:image" content="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQ8pUHqzXZyVQVzhiuA5_j12EAzIkBfZaBI98UplyqsogqvJ_4tkrCGNhF6l63AK7KLsG4&usqp=CAU">
    
    <meta property="twitter:card" content="summary">
    <!-- <meta property="twitter:url" content="https://bearblog.dev"> -->
    <meta property="twitter:title" content="◕ᴗ◕ Dragon Odile">
    <meta property="twitter:description" content={pageDescription}>
